%a.dropdown-toggle.display-flex.padding-10{"aria-controls" => "otherSections1", "aria-expanded" => "false", "data-toggle" => "collapse", :href => "#otherSections1", :role => "button" }
  #icon-web
  %span{:style => "margin-left:10px;"} Gui
%ul#otherSections1.collapse.list-unstyled.margin-left-60
  %li.display-flex.padding-10
    #icon-ruby
    %a#web-ruby.scroll-link.margin-left-10{:href => "#web/ruby/"} Ruby
  %li.display-flex.padding-10
    #icon-java
    %a#web-java.scroll-link.margin-left-10{:href => "#web/java"} Java
  %li.display-flex.padding-10
    #icon-js
    %a#web-js.scroll-link.margin-left-10{:href => "#web/js"} JS
  %br


    :javascript
      $(document).ready(function() {
        $("#web-ruby").click(function(e) {
            e.preventDefault();
            $.ajax({
            url: "/web/ruby",
            type: "get", data: { locale: '#{params[:locale]}'} });
        });
      });

      $(document).ready(function() {
        $("#web-java").click(function(e) {
            e.preventDefault();
            $.ajax({ url: "/web/java", type: "get", data: { locale: '#{params[:locale]}'} });
        });
      });

      $(document).ready(function() {
        $("#web-js").click(function(e) {
            e.preventDefault();
            $.ajax({ url: "/web/js", type: "get", data: { locale: '#{params[:locale]}'} });
        });
      });
